<!doctype,doctype>
<html>
  <head>
    <meta charset="utf-8">
    <title>Sum</title>
    <link rel="stylesheet" href="/css/sumstyle.css">
  </head>
  <body>
  <h1>SumList</h1>
  <table>
    <% for(var i = 0; i < list.length; i++){ %>
    <% var color = (list[i]%2) ? odd : even %>
    <tr>
      <td style="color: <%=color%>;">
        <%= list[i] %>
      </td>
    </tr>
    <% } %>
    <tr>
      <td> <%= sum %> </td>
    </tr>
  </table>
  </body>
</html>
